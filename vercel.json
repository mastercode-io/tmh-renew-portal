{
  "redirects": [
    { "source": "/", "destination": "https://www.thetrademarkhelpline.com", "permanent": false }
  ],
  "rewrites": [
    { "source": "/uktm", "destination": "/renewal/uktm/index.html" },
    { "source": "/uktm/index.html", "destination": "/renewal/uktm/index.html" },
    { "source": "/uktm/order", "destination": "/renewal/uktm/order.html" },
    { "source": "/uktm/order.html", "destination": "/renewal/uktm/order.html" },
    { "source": "/uktm/confirmation", "destination": "/renewal/uktm/confirmation.html" },
    { "source": "/uktm/confirmation.html", "destination": "/renewal/uktm/confirmation.html" },
    { "source": "/uktm/(.*)", "destination": "/renewal/uktm/$1" }
  ]
}
